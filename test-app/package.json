{
  "name": "test-app",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for test-app goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "types:watch": "glint --build --watch",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "concurrently 'ember serve' 'pnpm _syncPnpm --watch' --names 'serve,sync deps'",
    "_syncPnpm": "DEBUG=sync-pnpm pnpm sync-dependencies-meta-injected",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "embroider:start": "EMBROIDER_TEST_SETUP_FORCE=embroider EMBROIDER_TEST_SETUP_OPTIONS=optimized ember s",
    "embroider:test": "EMBROIDER_TEST_SETUP_FORCE=embroider EMBROIDER_TEST_SETUP_OPTIONS=optimized ember test",
    "try:embroider:start": "ember try:one embroider-optimized --- ember s",
    "try:embroider:test": "ember try:one embroider-optimized"
  },
  "dependencies": {
    "ember-modifier": "^3.2.7",
    "ember-velcro": "workspace:*"
  },
  "dependenciesMeta": {
    "ember-velcro": {
      "injected": true
    }
  },
